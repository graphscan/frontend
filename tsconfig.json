{
  "compilerOptions": {
    "target": "ES2023",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "styled.d.ts",
    ".next/types/**/*.ts"
, "src/components/profile/components/account-card/components/notification-modal/notification-modal.styled.ts.bkp", "src/components/profile/components/account-card/components/notification-modal/notification-modal.icons.tsx.bkp", "src/components/profile/components/account-card/components/notification-modal/notification-modal.component.tsx.bkp"  ],
  "exclude": ["node_modules"]
}
